{
  "name": "@strong-roots-capital/io-ts-aws",
  "version": "0.0.31",
  "description": "io-ts library for aws-sdk",
  "keywords": [
    "io-ts",
    "aws-sdk"
  ],
  "homepage": "https://github.com/strong-roots-capital/io-ts-aws",
  "bugs": {
    "url": "https://github.com/strong-roots-capital/io-ts-aws/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strong-roots-capital/io-ts-aws.git"
  },
  "license": "ISC",
  "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/EricCrosson)",
  "sideEffects": false,
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run compile",
    "ava": "ava -v",
    "clean": "rimraf --no-glob dist",
    "compile": "tsc",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "docs-ts",
    "lint": "eslint --ext ts .",
    "lint:fix": "eslint --ext ts --fix .",
    "lint-staged": "lint-staged",
    "prettier": "prettier --list-different src/**/*.ts test/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "test": "npm run prettier && npm run lint && nyc ava && npm run docs"
  },
  "dependencies": {
    "@aws-sdk/util-dynamodb": ">= 3.0.0"
  },
  "devDependencies": {
    "@ericcrosson/eslint-config": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "ava": "^3.14.0",
    "ava-fast-check": "^4.0.0",
    "codecov": "^3.8.1",
    "docs-ts": "^0.5.3",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ava": "^10.2.0",
    "eslint-plugin-fp-ts": "^0.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-security": "^1.4.0",
    "fast-check": "^2.10.0",
    "fp-ts": "^2.10.0",
    "lint-staged": "^10.5.3",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "io-ts": "~2.1.3",
    "io-ts-types": ">= 0.5.0",
    "monocle-ts": ">= 2.3.0",
    "newtype-ts": ">= 0.3.0"
  },
  "pre-commit": "lint-staged",
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "!dist/**/*.d.ts",
      "test/**/test-*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
